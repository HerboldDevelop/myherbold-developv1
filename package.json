    "lodash.flow": "^3.5.0",
    "myherbold-tolgee": "^1.55.0",
    "myherbold-translation": "^1.18.0",
    "prop-types": "^15.7.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x || 16.x || 17.x"
  },
  "devDependencies": {
